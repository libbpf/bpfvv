{
    "compilerOptions": {
        "target": "ES6",
        "module": "ESNext",
        "moduleResolution": "node",
        "outDir": "./dist",
        "rootDir": "./",
        "sourceMap": true,
        "inlineSources": true,
        "inlineSourceMap": false,
        "baseUrl": "./",
        "paths": {
            "*": ["node_modules/*"]
        }
    },
    "include": [
        "app.ts",
        "parser.ts"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}
